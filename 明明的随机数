#include <stdio.h>
#include <stdlib.h>

int main()
{
  // 请在此输入您的代码
  int num,eee[1001]={0},t,i;  //别忘了初始化数组为0
  int count = 0;
  scanf("%d",&num);
  for(i=0;i<num;i++){
    scanf("%d",&t);
    if(eee[t]==0){  //巧妙之处
      eee[t] = t;
      count++;
    }
  }
  printf("%d\n",count);
  for(i=0;i<1001;i++){
    if(eee[i]!=0){
      printf("%d ",eee[i]);
    }
  }
  return 0;
}
