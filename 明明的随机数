#include <stdio.h>
#include <stdlib.h>

int main()
{
  int num,a[1001]={0},t,i;
  int count = 0;
  scanf("%d",&num);
  for(i=0;i<num;i++)
  {
    scanf("%d",&t);
    if(a[t]==0)
    { 
      a[t] = t;
      count++;
    }
  }
  printf("%d\n",count);
  for(i=0;i<1001;i++)
  {
    if(a[i]!=0)
    {
      printf("%d ",a[i]);
    }
  }
  return 0;
}
